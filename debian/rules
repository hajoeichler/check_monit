#!/usr/bin/make -f
%:
	dh $@

override_dh_auto_install:
	mkdir -p $$(pwd)/debian/nagios-icinga/usr/lib/nagios/plugins
	cp check_remote_icinga.rb $$(pwd)/debian/nagios-icinga/usr/lib/nagios/plugins

	mkdir -p $$(pwd)/debian/nagios-icinga/etc/nagios-plugins/config/
	cp remote_icinga.cfg $$(pwd)/debian/nagios-icinga/etc/nagios-plugins/config/
